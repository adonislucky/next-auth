{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "next-auth#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "lib/**",
        "css/**",
        "jwt/**",
        "react/**",
        "next/**",
        "client/**",
        "providers/**",
        "core/**",
        "index.d.ts",
        "index.js",
        "adapters.d.ts",
        "middleware.d.ts",
        "middleware.js"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "@next-auth/upstash-redis-adapter#test": {
      "env": ["UPSTASH_REDIS_KEY", "UPSTASH_REDIS_URL"]
    }
  }
}
